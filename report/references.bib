	url = {https://linkinghub.elsevier.com/retrieve/pii/S1074761300802496},
	doi = {10.1016/S1074-7613(00)80249-6},
	abstract = {Destruction of Ii by proteolysis is required for MHC class II molecules to bind antigenic peptides, and for transport of the resulting complexes to the cell surface. The cysteine protease cathepsin S is highly expressed in spleen, lymphocytes, monocytes, and other class II–positive cells, and is inducible with interferon-␥. Specific inhibition of cathepsin S in B lymphoblastoid cells prevented complete proteolysis of Ii, resulting in accumulation of a class II–associated 13 kDa Ii fragment in vivo. Consequently, the formation of SDS-stable complexes was markedly reduced. Purified cathepsin S, but not cathepsin B, H, or D, specifically digested Ii from ␣␤Ii trimers, generating ␣␤–CLIP complexes capable of binding exogenously added peptide in vitro. Thus, cathepsin S is essential in B cells for effective Ii proteolysis necessary to render class II molecules competent for binding peptides.},
	language = {en},
	number = {4},
	urldate = {2019-10-14},
	journal = {Immunity},
	author = {Riese, Richard J and Wolf, Paula R and Brömme, Dieter and Natkin, Lisa R and Villadangos, José A and Ploegh, Hidde L and Chapman, Harold A},
	month = apr,
	year = {1996},
	note = {14},
	pages = {357--366},
}

@article{shi_cathepsin_1999,
	title = {Cathepsin {S} {Required} for {Normal} {MHC} {Class} {II} {Peptide} {Loading} and {Germinal} {Center} {Development}},
	volume = {10},
	issn = {10747613},
	url = {https://linkinghub.elsevier.com/retrieve/pii/S1074761300800205},
	doi = {10.1016/S1074-7613(00)80020-5},
	abstract = {Major histocompatibility complex (MHC) class II molecules acquire antigenic peptides after degradation of the invariant chain (Ii), an MHC class II–associated protein that otherwise blocks peptide binding. Antigen-presenting cells of mice that lack the protease cathepsin S fail to process Ii beyond a 10 kDa fragment, resulting in delayed peptide loading and accumulation of cell surface MHC class II/10 kDa Ii complexes. Although cathepsin S–deficient mice have normal numbers of B and T cells and normal IgE responses, they show markedly impaired antibody class switching to IgG2a and IgG3. These results indicate cathepsin S is a major Ii-processing enzyme in splenocytes and dendritic cells. Its role in humoral immunity critically depends on how antigens access the immune system.},
