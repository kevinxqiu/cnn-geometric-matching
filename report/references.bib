	author = {Shimabukuro-Vornhagen, Alexander and García-Márquez, María and Fischer, Rieke N. and Iltgen-Breburda, Juliane and Fiedler, Anne and Wennhold, Kerstin and Rappl, Gunter and Abken, Hinrich and Lehmann, Clara and Herling, Marco and Wolf, Dominik and Fätkenheuer, Gerd and Rubbert-Roth, Andrea and Hallek, Michael and Theurich, Sebastian and von Bergwelt-Baildon, Michael},
